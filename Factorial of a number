import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
		long fact=1, num;
    	
    	Scanner input = new Scanner(System.in);
    	System.out.println("enter number to find factorial");
    	num = input.nextInt();
    	
    	while(num>0){
    	    fact = fact * num;
    	    num = num - 1;
    	}
    	System.out.println(fact);
	}
}
